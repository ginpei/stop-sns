{
  "manifest_version": 2,
  "name": "Stop SNS",
  "version": "1.0",
  "description": "Help you to spend time on SNS.",
  "icons": {
    "48": "icon-512.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "content_scripts/index.js"
      ],
      "css": [
        "content_scripts/main.css"
      ]
    }
  ]
}
